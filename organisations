<script>
    $(document).ready(function() {
        // handle the mouseenter functionality
        $(".img").mouseenter(function() {
                $(this).addClass("hover");
            })
            // handle the mouseleave functionality
            .mouseleave(function() {
                $(this).removeClass("hover");
            });
        });

        $("div.container-slider").click(function() {
            var current_div = $(this).children('div').attr('id');
            var current_id = $(this).attr('id');

            $("#kiran_slide").attr('class', 'kiran inactive-slide');
            $("#ben_slide").attr('class', 'ben inactive-slide');
            $("#lilia_slide").attr('class', 'lilia inactive-slide');

            $('.testimonial-text').hide();

            if (current_id == 'item1') {
                if ($('.active-slide').hasClass('animate')) {
                    $('.active-slide').removeClass('animate', 600);
                }

                if ($('.active-slide').hasClass('animate_to_third')) {
                    $('.active-slide').removeClass('animate_to_third', 600);
                }

                $("#kiran_slide").attr('class', 'kiran current-font');
                $("#students").attr('class', 'testimonial-slider kiran-slider');
                $("#kiran_testimonial").show(function() {
                    $(this).slideDown(1000);
                });
            } else if (current_div == 'ben_slide') {
                if ($('.active-slide').hasClass('animate_to_third')) {
                    $('.active-slide').addClass('animate');
                    $('.active-slide').removeClass('animate_to_third', 600, function() {

                    });
                }

                $("#ben_slide").attr('class', 'ben current-font');
                $("#students").attr('class', 'testimonial-slider ben-slider');
                $('.active-slide').addClass('animate', 600);

                $("#ben_testimonial").show(function() {
                    $(this).slideDown(1000);
                });
            } else {
                $('.active-slide').addClass('animate_to_third', 600);

                if ($('.active-slide').hasClass('animate')) {
                    $('.active-slide').removeClass('animate');
                }

                $("#lilia_slide").attr('class', 'lilia current-font');
                $("#students").attr('class', 'testimonial-slider lilia-slider');
                $("#lilia_testimonial").show(function() {
                    $(this).slideDown(1000);
                });
            }
        });
    </script>
</html>